{
	"atomKeymap.promptV3Features": true,
	"debug.console.fontSize": 16,
	"window.zoomLevel": -0.5,
	"window.openFoldersInNewWindow": "on",
	"workbench.startupEditor": "none",
	"editor.bracketPairColorization.enabled": true,
	"editor.codeActionsOnSave": ["source.fixAll.eslint"],
	"editor.cursorBlinking": "smooth",
	"editor.fontFamily": "\"Fira Code\", Menlo, Monaco, ‘Courier New’, monospace",
	"editor.fontLigatures": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.inlineSuggest.enabled": true,
	"editor.lineNumbers": "relative",
	"editor.minimap.enabled": false,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.showFoldingControls": "always",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.suggestFontSize": 16,
	"editor.suggestSelection": "first",
	"editor.tabSize": 2,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"files.associations": {
		"*.jsx": "typescriptreact",
		".env*": "env"
	},
	"files.trimTrailingWhitespace": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true
	},
	"git.autofetch": true,
	"git.experimental.mergeEditor": true,
	"git.confirmSync": false,
	"gitlens.currentLine.scrollable": false,
	"gitlens.views.worktrees.files.layout": "list",
	"github.copilot.enable": {
		"*": true,
		"plaintext": true,
		"markdown": true,
		"scminput": false,
		"yaml": true,
		"haskell": false,
		"typescript": true,
		"ini": true,
		"rust": true
	},
	"githubPullRequests.pullBranch": "never",
	"github.copilot.editor.enableAutoCompletions": true,
	"emmet.includeLanguages": {
		"haskell": "html"
	},
	"emmet.syntaxProfiles": {
		"javascript": "jsx",
		"typescript": "tsx"
	},
	"haskell.formattingProvider": "stylish-haskell",
	"terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"terminal.integrated.fontSize": 16,
	"todohighlight.keywords": [
		{
			"text": "TODO:",
			"color": "red",
			"border": "1px solid red",
			"borderRadius": "2px",
			"backgroundColor": "rgba(0,0,0,.2)"
		},
		{
			"text": "DEPRECATED:",
			"color": "#D358F7",
			"border": "1px solid #D358F7",
			"borderRadius": "2px",
			"backgroundColor": "rgba(0,0,0,.2)"
		}
	],
	"vim.easymotion": true,
	"vim.foldfix": true,
	"vim.hlsearch": true,
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		}
	],
	"vim.leader": "space",
	"vim.replaceWithRegister": true,
	"vim.smartRelativeLine": true,
	"vim.sneak": true,
	"vim.useSystemClipboard": true,
	"vim.visualstar": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["leader", "k"],
			"after": ["leader", "leader", "k"]
		},
		{
			"before": ["leader", "j"],
			"after": ["leader", "leader", "j"]
		},
		{
			"before": ["s"],
			"after": ["leader", "leader", "s"]
		}
	],
	"vim.easymotionKeys": "qwertasdfgzxcvbuiopyhjklnm,;",
	"vim.normalModeKeyBindings": [
		{
			"before": ["C-w", "v"],
			"after": [":", "v", "s", "<Enter>"]
		},
		{
			"before": ["C-w", "s"],
			"after": [":", "s", "p", "<Enter>"]
		}
	],
	"workbench.colorCustomizations": {
		"statusBar.background": "#005f5f",
		"statusBar.noFolderBackground": "#005f5f",
		"statusBar.debuggingBackground": "#005f5f"
	},
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.editor.tabSizing": "shrink",
	"workbench.editor.wrapTabs": true,
	"workbench.editor.splitSizing": "distribute",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.localHistory.enabled": false,
	"workbench.sideBar.location": "left",
	"[haskell]": {
		"editor.defaultFormatter": "haskell.haskell"
	},
	"deno.path": "/nix/store/6xvnjz2grdah9kawh9qcf2d6gh0pda23-deno-1.16.2/bin/deno",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"prettier.useTabs": true,
	"rust.checkOnSave": true,
	"rust.formatOnSave": true,
	"typescript.referencesCodeLens.enabled": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"totalTypeScript.hideAllTips": true,
	"julia.symbolCacheDownload": true,
	"editor.accessibilitySupport": "off",
	"julia.enableTelemetry": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"importCost.timeout": 700,
	"errorLens.delay": 1000,
	"diffEditor.experimental.showMoves": true,
	"security.promptForLocalFileProtocolHandling": false,
	"typescript.preferences.importModuleSpecifier": "relative",
	"workbench.colorTheme": "One Dark Pro",
	"remoteHub.search.download.enabled": true,
	"tabnine.experimentalAutoImports": true,
	"hadolint.hadolintPath": "/nix/store/qxng2a025jiv5qran4z7bfc7qwvdnc6l-hadolint",
	"[python]": {
		"diffEditor.ignoreTrimWhitespace": true
	},
	"window.commandCenter": false,
	"window.title": "${activeFolderMedium}${separator}${rootName}",
	"intelephense.environment.phpVersion": "8.1.0",
	"nix.enableLanguageServer": true,
	"nix.formatterPath": "nixfmt",
	"notebook.editorOptionsCustomizations": {
		"editor.tabSize": 4,
		"editor.indentSize": 4,
		"editor.insertSpaces": true
	},
	"editor.inlayHints.enabled": "offUnlessPressed",
	"[dockerfile]": {
		"editor.defaultFormatter": "ms-azuretools.vscode-docker"
	},
	"haskell.manageHLS": "PATH",
	"cursor.cpp.disabledLanguages": ["scminput", "haskell"],
	"cursor.diffs.useCharacterLevelDiffs": true,
	"cursor.terminal.usePreviewBox": true,
	"update.releaseTrack": "prerelease",
	"yaml.schemas": {
		"kubernetes": ["*.yml", "*.yaml"]
	},
	"commentTranslate.targetLanguage": "ja"
}
