{
	"title": "Shift-FnでGyazo GIF起動",
	"rules": [
		{
			"description": "Shift-FnでGyazo GIF起動",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "fn",
						"modifiers": { "mandatory": ["shift"] }
					},
					"to_if_alone": [
						{
							"shell_command": "open -a 'Gyazo GIF.app'"
						}
					]
				}
			]
		}
	]
}
